# Backend URL for API requests and file downloads
# This is used for direct file downloads and API calls
VITE_BACKEND_URL=http://localhost:1337

# Public URL for the frontend (used for redirects, etc.)
VITE_PUBLIC_URL=http://localhost:5173

# API base path (relative to frontend, gets proxied to backend)
VITE_API_BASE=/api

# Example production configuration:
# VITE_BACKEND_URL=https://your-backend-domain.com
# VITE_PUBLIC_URL=https://your-frontend-domain.com
